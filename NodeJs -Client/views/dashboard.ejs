       <script type="text/javascript">
          function profileRender(id,product_id)
          {    
            var idValue = document.getElementById('user_id').value;                
            $.ajax({            
              type: "GET",
              contentType: 'application/json',
              url:"http://localhost:3000/users/"+idValue,        
              success: function(data){
                console.log('inside the function profile');
                var parentDiv = document.createElement('div');
                parentDiv.setAttribute('id','portfolio'); 
                parentDiv.setAttribute('class',"bg-light-gray"); 
