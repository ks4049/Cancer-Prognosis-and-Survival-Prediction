            //             <i class="fa fa-circle fa-stack-2x text-primary"></i>
            //             <i class="fa fa-user fa-stack-1x fa-inverse"></i>
            //    </span>
            // </div>   

            var containerTag=document.createElement('div');
            containerTag.setAttribute('class',"container");
            var divTag = document.createElement('div');
            divTag.setAttribute('class',"row");
            var childTag1 = document.createElement('div');
            childTag1.setAttribute('class',"col-lg-12 text-center"); 
            divTag.appendChild(childTag1);
            var hTag=document.createElement('h2');
            hTag.setAttribute('class',"section-heading");
            var htagText = document.createTextNode("Profile");
            hTag.appendChild(htagText);
            childTag1.appendChild(hTag);
            var profileTag = document.createElement('div');
            var spanTag=document.createElement('span');
            spanTag.setAttribute('class',"fa-stack fa-4x");
            var iTag = document.createElement('i');
            iTag.setAttribute('class',"fa fa-circle fa-stack-2x text-primary");
            spanTag.appendChild(iTag);
            var iTag1 = document.createElement('i');
            iTag1.setAttribute('class',"fa fa-user fa-stack-1x fa-inverse");
            spanTag.appendChild(iTag1);
            containerTag.appendChild(divTag);
            containerTag.appendChild(spanTag);
            parentDiv.appendChild(containerTag);
                document.body.insertBefore(parentDiv,document.getElementById('contact'));
                var userNameDiv = document.createElement('div');
                userNameDiv.setAttribute('class',"paddingStyle");
                var userName  = document.createTextNode("UserName: "+data.username);
                userNameDiv.appendChild(userName);
                var emailTag = document.createElement('div');
                emailTag.setAttribute('class',"paddingStyle");
                var emailText = document.createTextNode("Email : "+data.email);
                emailTag.appendChild(emailText);
                var genderTag = document.createElement('div');
                genderTag.setAttribute('class',"paddingStyle");
                var genderText = document.createTextNode("Gender : "+data.gender);
                genderTag.appendChild(genderText);
                var city = document.createElement('div');
                city.setAttribute('class',"paddingStyle");
                var cityText = document.createTextNode("City : "+data.city);
                city.appendChild(cityText);
                var stateTag = document.createElement('div');
                stateTag.setAttribute('class',"paddingStyle");
                var stateText = document.createTextNode("State : "+data.state);
                stateTag.appendChild(stateText);
                var nationalityTag = document.createElement('div');
                nationalityTag.setAttribute('class',"paddingStyle");
                var nationalityText = document.createTextNode("Nationality : "+data.nationality);
                nationalityTag.appendChild(nationalityText);
                parentDiv.appendChild(userNameDiv);
                parentDiv.appendChild(emailTag);
                parentDiv.appendChild(genderTag);
                parentDiv.appendChild(city);
                parentDiv.appendChild(stateTag);
                parentDiv.appendChild(nationalityTag);              
                return data;
              }              
